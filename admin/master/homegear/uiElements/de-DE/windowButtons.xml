<?xml version="1.0" encoding="utf-8"?>
<homegearUiElements lang="de-DE">
  <homegearUiElement>
    <uniqueUiElementId>Base.windowButtons</uniqueUiElementId>
    <control>windowButtons</control>
    <type>simple</type>
    <description>Stellt Fensterknöpfe für komplexe UI-Elemente zur Verfügung.</description>
    <texts>
      <text id="title">
        <content>Trigger</content>
        <color>title</color>
      </text>
      <text id="l2_state_title">
        <content>Status</content>
        <color>l2_state_title</color>
      </text>
    </texts>
    <icons>
      <icon id="l2">
        <name>switches_1</name>
        <color>inactive</color>
      </icon>
      <icon id="l3">
        <name>arrow_right_1</name>
        <color>iconArrow</color>
      </icon>
    </icons>
    <variableInputs>
      <variable>
        <label>-</label>
        <description>Ungenutzt</description>
        <deviceTypeId>*</deviceTypeId>
        <channel>*</channel>
        <name>*</name>
        <visualizeInOverview>false</visualizeInOverview>
        <rendering>
          <condition operator="not" value="0">
            <icons>
              <icon id="state">
                <name>arrow_up_1</name>
                <color>active</color>
              </icon>
            </icons>
            <texts>
              <text id="state">
                <content>an</content>
                <color>active</color>
              </text>
            </texts>
          </condition>
          <condition operator="e" value="0">
            <icons>
              <icon id="state">
                <name>arrow_up_1</name>
                <color>inactive</color>
              </icon>
            </icons>
            <texts>
              <text id="state">
                <content>aus</content>
                <color>inactive</color>
              </text>
            </texts>
          </condition>
        </rendering>
      </variable>
      <variable>
        <label>-</label>
        <description>Ungenutzt</description>
        <deviceTypeId>*</deviceTypeId>
        <channel>*</channel>
        <name>*</name>
        <visualizeInOverview>false</visualizeInOverview>
        <rendering>
          <condition operator="not" value="0">
            <icons>
              <icon id="state">
                <name>close_x_1</name>
                <color>active</color>
              </icon>
            </icons>
            <texts>
              <text id="state">
                <content>an</content>
                <color>active</color>
              </text>
            </texts>
          </condition>
          <condition operator="e" value="0">
            <icons>
              <icon id="state">
                <name>close_x_1</name>
                <color>inactive</color>
              </icon>
            </icons>
            <texts>
              <text id="state">
                <content>aus</content>
                <color>inactive</color>
              </text>
            </texts>
          </condition>
        </rendering>
      </variable>
      <variable>
        <label>-</label>
        <description>Ungenutzt</description>
        <deviceTypeId>*</deviceTypeId>
        <channel>*</channel>
        <name>*</name>
        <visualizeInOverview>false</visualizeInOverview>
        <rendering>
          <condition operator="not" value="0">
            <icons>
              <icon id="state">
                <name>arrow_down_1</name>
                <color>active</color>
              </icon>
            </icons>
            <texts>
              <text id="state">
                <content>an</content>
                <color>active</color>
              </text>
            </texts>
          </condition>
          <condition operator="e" value="0">
            <icons>
              <icon id="state">
                <name>arrow_down_1</name>
                <color>inactive</color>
              </icon>
            </icons>
            <texts>
              <text id="state">
                <content>aus</content>
                <color>inactive</color>
              </text>
            </texts>
          </condition>
        </rendering>
      </variable>
    </variableInputs>
    <variableOutputs>
      <variable>
        <label>Hoch</label>
        <description>Der Hoch-Knopf wurde angeclickt.</description>
        <types>bool</types>
        <deviceTypeId>*</deviceTypeId>
        <channel>*</channel>
        <name>*</name>
      </variable>
      <variable>
        <label>Stopp</label>
        <description>Der Stopp-Knopf wurde angeclickt.</description>
        <types>bool</types>
        <deviceTypeId>*</deviceTypeId>
        <channel>*</channel>
        <name>*</name>
      </variable>
      <variable>
        <label>Runter</label>
        <description>Der Runter-Knopf wurde angeclickt.</description>
        <types>bool</types>
        <deviceTypeId>*</deviceTypeId>
        <channel>*</channel>
        <name>*</name>
      </variable>
    </variableOutputs>
  </homegearUiElement>
</homegearUiElements>
