<?xml version="1.0" encoding="utf-8"?>
<homegearUiElements lang="de-DE">
  <homegearUiElement>
    <uniqueUiElementId>Base.windowPosition</uniqueUiElementId>
    <control>windowPosition</control>
    <type>simple</type>
    <description>Stellt komplexen Elementen einen Schieberegler zum Visualisieren und Setzen der Fensterposition zur Verfügung.</description>
    <texts>
      <text id="title">
        <content>Position</content>
        <color>title</color>
      </text>
      <text id="l2_state_title">
        <content>Position</content>
        <color>l2_state_title</color>
      </text>
    </texts>
    <icons>
      <icon id="l2">
        <name>window_2</name>
        <color>inactive</color>
      </icon>
      <icon id="l3">
        <name>arrow_right_1</name>
        <color>iconArrow</color>
      </icon>
    </icons>
    <variableInputs>
      <variable>
        <label>Position</label>
        <description>Setzt die visualisierte Position.</description>
        <types>int</types>
        <deviceTypeId>*</deviceTypeId>
        <channel>*</channel>
        <name>*</name>
        <visualizeInOverview>true</visualizeInOverview>
        <unit>%</unit>
        <minimumValue>0</minimumValue>
        <maximumValue>100</maximumValue>
        <minimumValueScaled>0</minimumValueScaled>
        <maximumValueScaled>100</maximumValueScaled>
      </variable>
    </variableInputs>
    <variableOutputs>
      <variable>
        <label>Position</label>
        <description>Gibt Positionsänderungen des UI-Elements aus.</description>
        <types>int</types>
        <deviceTypeId>*</deviceTypeId>
        <channel>*</channel>
        <name>*</name>
      </variable>
    </variableOutputs>
  </homegearUiElement>
</homegearUiElements>
