<?xml version="1.0" encoding="utf-8"?>
<homegearUiElements lang="de-DE">
  <homegearUiElement>
    <uniqueUiElementId>Base.windowButtonsUpDown</uniqueUiElementId>
    <control>windowButtonsUpDown</control>
    <type>simple</type>
    <texts>
      <text id="title">
        <content>Schalter</content>
        <color>title</color>
      </text>
      <text id="l2_state_title">
        <content>Status</content>
        <color>l2_state_title</color>
      </text>
    </texts>
    <icons>
      <icon id="l2">
        <name>window_2</name>
        <color>inactive</color>
      </icon>
      <icon id="l3">
        <name>arrow_right_1</name>
        <color>iconArrow</color>
      </icon>
    </icons>
    <variableInputs>
      <variable>
        <deviceTypeId>*</deviceTypeId>
        <channel>*</channel>
        <name>*</name>
        <visualizeInOverview>false</visualizeInOverview>
        <rendering>
          <condition operator="not" value="0">
            <icons>
              <icon id="state">
                <name>arrow_up_1</name>
                <color>active</color>
              </icon>
            </icons>
            <texts>
              <text id="state">
                <content>an</content>
                <color>active</color>
              </text>
            </texts>
          </condition>
          <condition operator="e" value="0">
            <icons>
              <icon id="state">
                <name>arrow_up_1</name>
                <color>inactive</color>
              </icon>
            </icons>
            <texts>
              <text id="state">
                <content>aus</content>
                <color>inactive</color>
              </text>
            </texts>
          </condition>
        </rendering>
      </variable>
      <variable>
        <deviceTypeId>*</deviceTypeId>
        <channel>*</channel>
        <name>*</name>
        <visualizeInOverview>false</visualizeInOverview>
        <rendering>
          <condition operator="not" value="0">
            <icons>
              <icon id="state">
                <name>arrow_down_1</name>
                <color>active</color>
              </icon>
            </icons>
            <texts>
              <text id="state">
                <content>an</content>
                <color>active</color>
              </text>
            </texts>
          </condition>
          <condition operator="e" value="0">
            <icons>
              <icon id="state">
                <name>arrow_down_1</name>
                <color>inactive</color>
              </icon>
            </icons>
            <texts>
              <text id="state">
                <content>aus</content>
                <color>inactive</color>
              </text>
            </texts>
          </condition>
        </rendering>
      </variable>
    </variableInputs>
    <variableOutputs>
      <variable>
        <label>Hoch</label>
        <description>Der Hoch-Knopf wurde angeklickt.</description>
        <types>bool</types>
        <deviceTypeId>*</deviceTypeId>
        <channel>*</channel>
        <name>*</name>
      </variable>
      <variable>
        <label>Runter</label>
        <description>Der Runter-Knopf wurde angeklickt.</description>
        <types>bool</types>
        <deviceTypeId>*</deviceTypeId>
        <channel>*</channel>
        <name>*</name>
      </variable>
    </variableOutputs>
    <metadata>
      <l3_force>true</l3_force>
    </metadata>
  </homegearUiElement>
</homegearUiElements>
